{
  "expo": {
    "name": "dinoCine",
    "slug": "dinoCine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dinoCineLogo.png",
    "scheme": "dinocine",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.dinocine.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/dinoCineLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#A2CC9C",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 300
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
